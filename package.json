{
  "name": "scss-generator-x",
  "displayName": "scss generator x",
  "description": "generate scss markup from your html and vue SFCs",
  "version": "1.0.0",
  "icon": "images/logo.png",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Snippets",
    "Formatters",
    "Programming Languages",
    "Linters",
    "Keymaps",
    "Other"
  ],
  "keywords": [
    "generate",
    "generator",
    "auto",
    "automatic",
    "css",
    "scss",
    "sass",
    "html",
    "htm",
    "vue",
    "vue2.0",
    "vue3.0",
    "vue2.x",
    "vue3.x",
    "SFCs",
    "easy scss",
    "easy sass",
    "compile",
    "style",
    "compiler"
  ],
  "activationEvents": [
    "onLanguage:vue",
    "onLanguage:scss"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": []
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.4",
    "@types/lodash": "^4.14.200",
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vscode/test-electron": "^2.3.6",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "jsdom": "^22.1.0",
    "lodash": "^4.17.21",
    "utilizes": "^1.3.45",
    "vue-sfc-parser": "^0.1.2"
  }
}
